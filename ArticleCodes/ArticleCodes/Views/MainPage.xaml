<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
             prism:ViewModelLocator.AutowireViewModel="True"
             x:Class="ArticleCodes.Views.MainPage"
             Title="MainPage">
  <StackLayout HorizontalOptions="Center" VerticalOptions="Center">
    <Button Text="Go TO Notification On Off"
            Command="{Binding NavigationToNotificationCommand}" />
    <Button Text="Go TO Overray On Off"
            Command="{Binding NavigationToOverlayCommand}" />
    <Button Text="Go TO UsagePage"
            Command="{Binding NavigationToUsageCommand}" />
  </StackLayout>
</ContentPage>